<h1>Editing Book</h1>
  <%= form_with model: @book, url: update_book_path(@book.id), method: :patch do |f| %>
    <div>
      <label>Title</label></div>
      <%= f.text_field :title %>
    <div>
      <label>Body</label></div>
      <%= f.text_area :body %>
    <div><%= f.submit 'Create Book' %></div>
  <% end %>
  
